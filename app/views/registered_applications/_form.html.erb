<%= form_for @application do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control', placeholder: "Enter the Name" %>
  </div>
  <div class="form-group">
    <%= f.label :url %>
    <%= f.text_field :url, rows: 8, class: 'form-control', placeholder: "Enter the Url" %>
  </div>
  <%= f.submit "Save", class: 'btn btn-success' %>
<% end %>
